{
  "family": "appointment-service",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "appointment-container",
      "image": "node:18-alpine",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}